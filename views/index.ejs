
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title><%= title %></title>
  <!-- GSAP CDN -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  <link rel="stylesheet" href="/stylesheets/style.css">

</head>
<body>
  <!-- Award Section -->
  <section class="heading">
    <div class="container">
      <h1><%= awardTitle %></h1>
      <p><%= awardSubtitle %></p>
    </div>
  </section>

  <!-- Featured Projects Section -->
 <!-- Projects Section -->
<section id="projects">
  <div class="container">
    <h2>Featured Projects</h2>
    <div class="project-grid">
      <% projects.forEach(function(project) { %>
        <div class="project">
          <img src="<%= project.image %>" alt="<%= project.name %>">
          <div class="project-info">
            <h3><%= project.name %></h3>
            <p><%= project.category %> | <%= project.year %></p>
          </div>
        </div>
      <% }); %>
    </div>
  </div>
</section>

 <!-- About Section -->
<section class="about-section">
  <div class="container">
      <div class="about-content">
          <div class="about-text">
              <h2 class="section-title">About Essajee's Atelier</h2>
              <p class="about-description">
                  Essajee's Atelier is a boutique design studio specializing in creating bespoke 
                  interiors that blend timeless elegance with modern functionality. With over two 
                  decades of experience, we transform spaces into captivating environments that 
                  reflect our clients' unique personalities and lifestyles.
              </p>
              <div class="about-stats">
                  <div class="stat-item">
                      <span class="stat-number" data-count="20">0</span>+
                      <span class="stat-label">Years Experience</span>
                  </div>
                  <div class="stat-item">
                      <span class="stat-number" data-count="150">0</span>+
                      <span class="stat-label">Projects Completed</span>
                  </div>
                  <div class="stat-item">
                      <span class="stat-number" data-count="12">0</span>
                      <span class="stat-label">International Awards</span>
                  </div>
              </div>
          </div>
          <div class="about-image">
              <img src="<%= about.image %>" alt="About Essajee's Atelier">
          </div>
      </div>
  </div>
</section>


