<!-- Seamless Experience Section -->
<section id="seamless">
  <div class="container">
    <h2>A Seamless Experience</h2>
    <div class="seamless-images">
      <div class="seamless-image">
        <img src="<%= seamless.before %>" alt="Before">
        <div class="overlay">Before</div>
      </div>
      <div class="seamless-image">
        <img src="<%= seamless.after %>" alt="After">
        <div class="overlay">After</div>
      </div>
    </div>
    <p class="seamless-text">
      While we do the heavy lifting, all you need to do is walk into your brand new space.
    </p>
  </div>
</section>




